{
  "headers": [
    {
      "source": "/assets/bg/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2678400, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*\\.(jpg|jpeg|png|webp|avif))",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
